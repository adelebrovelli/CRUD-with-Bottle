{
    "version": 2,
    "builds": [
      {
        "src": "index.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.13" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.py"
      },
      {
        "src": "/favicon.ico",
        "dest": ".../favicon.ico"
      }
      
    ]
  }
  